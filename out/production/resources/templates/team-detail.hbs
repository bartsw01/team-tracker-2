{{#partial "content"}}
   <p>Team Name:</p>
   <p>{{team.teamName}}</p>
   <h5>Team Description:</h5>
   <p> {{team.teamDescription}}</p>
   <h5>Team Member 1:</h5>
   <p> {{team.teamMember1}}</p>
   <h5>Team Member 2:</h5>
   <p> {{team.teamMember2}}</p>
   <h5>Team Member 3</h5>
   <p> {{team.teamMember3}}</p>
   <h5>Team Member 4</h5>
   <p> {{team.teamMember4}}</p>
   <p>
       {{#if team.addedTeam}}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Added
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Pending
       {{/if}}

       <a href="/teams/{{team.id}}/update">Edit this team</a>
       <a href="/teams/{{team.id}}/delete">DELETE this team (no undo)</a>
   </p>
{{/partial}}
{{> layout.hbs}}